# .bash_profile file to handle aliases within OSX
# created by philipp necker

# ls aliases
alias la='ls -al'
alias ll='la -l'
alias sl='ls'

# cd aliases
alias cdp='cd ~/Documents/Projekte'
alias cdd='cd ~/Downloads'
alias cdpx='cd ~/Documents/Projekte/XCodeProjekte'

# various aliases
alias openhere='open .'
alias box='open ~/Dropbox'
alias downloads='open ~/Downloads'
alias documents='open ~/Documents'
